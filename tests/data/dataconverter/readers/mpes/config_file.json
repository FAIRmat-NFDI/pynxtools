  "/ENTRY[entry]/data": {
    "@axes": "@data:dims",
    "AXISNAME_indices[@*_indices]": "@data:*.index",
    "@signal": "data",
    "data": "@data:data",
    "data/@units": "counts",
    "AXISNAME[*]": "@data:*.data",
    "AXISNAME[*]/@units": "@data:*.unit",
    "energy/@type": "binding"
  }
