{
  "@default":"data",
  "/ENTRY/title": "@eln",
  "/ENTRY/USER[user]": {
    "name":"@eln",
    "address":"@eln"
  },
  "/ENTRY/INSTRUMENT[instrument]": {
    "@version":"@attrs:metadata/instrument/version",
    "DETECTOR[detector]":{
      "count_time":"@attrs:metadata/instrument/detector/count_time",
      "count_time/@units":"@attrs:metadata/instrument/detector/count_time_units"
    }
  },
  "/ENTRY/SAMPLE[sample]": {
    "name":"@eln",
    "physical_form":"@eln",
    "temperature":"@eln",
    "temperature/@units":"@eln"
  },
  "/ENTRY/data": {
    "@axes":["x_values", "y_values"],
    "@signal":"itensity",
    "AXIS[*]":"@data:*.axes",
    "AXIS[*]/@units":"@attrs:data/*/@units",
    "DATA[*]":"@data:*.data",
    "DATA[*]/@units":"@attrs:data/*/@units",
  }
}